<div id="agent{id}-div" class="agent-div">
    <table class="table">
        <tr>
            <td class="agent-title" colspan="4">
                SLOT {id}
            </td>
        </tr>
        <tr>
            <td class="agent-name-label">
                <strong>Agent :</strong>
            </td>
            <td>
                <select id="agent{id}-id" title="Please select agent">
                </select>
            </td>
        </tr>
        <tr>
            <td class="agent-level-label">
                <strong>Level :</strong>
            </td>
            <td>
                <select id="agent{id}-level" title="Please select agent">
                    <option value="25">25</option>
                    <option value="50">50</option>
                </select>
            </td>
        </tr>
    </table>
    <hr>
    <p id="agent{id}-text25">Empty agent slot</p>
    <p id="agent{id}-text50"></p>
</div>
